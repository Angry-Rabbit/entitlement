{
  "entry": "src/index.js",
  "extraBabelPlugins": [
    "transform-runtime",
    "dva-hmr",
    ["import", { "libraryName": "antd", "libraryDirectory": "lib", "style": "css" }]
  ],
  "proxy": {
    "/api/**": {
      "target": "http://192.168.51.168:8080/",
      "changeOrigin": true,
      "pathRewrite": { "^/api" : "" }
    },
    "/captcha": {
      "target": "http://192.168.50.230:9000/captcha",
      "changeOrigin": true,
      "pathRewrite": { "^/captcha" : "" }
    }
  },
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr",
        "transform-runtime"
      ]
    },
    "production": {
      "extraBabelPlugins": [
        "transform-runtime"
      ]
    }
  }
}
